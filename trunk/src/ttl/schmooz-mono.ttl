@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .

<http://studionumbersix.com/foo/lv2/schmooz-mono>
	a		lv2:CompressorPlugin;
	doap:name	"Schmooz Compressor (mono)";
	doap:license	<http://usefulinc.com/doap/licenses/gpl>;
	doap:maintainer [ foaf:name "Sampo Savolainen" ; ] ;
	ui:ui		<http://studionumbersix.com/foo/lv2/schmooz-mono/ui>;


	lv2:port [
		a lv2:AudioPort, lv2:InputPort;
		lv2:index 0;
		lv2:symbol "input";
		lv2:name "Input";
	],
	[
		a lv2:AudioPort, lv2:OutputPort;
		lv2:index 1;
		lv2:symbol "output";
		lv2:name "Output";
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 2;
		lv2:symbol "threshold";
		lv2:name "Threshold (dB)";
		lv2:minimum -60.0;
		lv2:maximum  10.0;
		lv2:default -10.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 3;
		lv2:symbol "sidechain_enabled";
		lv2:name "Sidechain HPF";
	        lv2:portProperty lv2:toggled ;

		lv2:minimum 0.0;
		lv2:maximum 1.0;
		lv2:default 0.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 4;
		lv2:symbol "attack_ms";
		lv2:name "Attack (ms)";
		lv2:minimum   0.1;
		lv2:maximum 120.0;
		lv2:default  10.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 5;
		lv2:symbol "release_ms";
		lv2:name "Release (ms)";
		lv2:minimum   50.0;
		lv2:maximum 1200.0;
		lv2:default  300.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 6;
		lv2:symbol "compression_ratio";
		lv2:name "Compression ratio (x:1)";
		lv2:minimum  1.5;
		lv2:maximum 20.0;
		lv2:default  5.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 7;
		lv2:symbol "makeup_gain_db";
		lv2:name "Makeup gain (dB)";
		lv2:minimum  0.0;
		lv2:maximum 40.0;
		lv2:default  0.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 8;
		lv2:symbol "dry_wet_balance";
		lv2:name "Dry/wet";
		lv2:minimum 0.0;
		lv2:maximum 1.0;
		lv2:default 1.0;
	],
  	[
		a lv2:ControlPort, lv2:OutputPort;
		lv2:index 9;
		lv2:symbol "attenuation";
		lv2:name "Attenuation";
		lv2:minimum -70.0;
		lv2:maximum  20.0;
	],
  	[
		a lv2:ControlPort, lv2:OutputPort;
		lv2:index 10;
		lv2:symbol "input_power";
		lv2:name "Input power";
		lv2:minimum -60.0;
		lv2:maximum  10.0;
	],
  	[
		a lv2:ControlPort, lv2:OutputPort;
		lv2:index 11;
		lv2:symbol "compressed_power";
		lv2:name "Compressed power";
		lv2:minimum -60.0;
		lv2:maximum  10.0;
	].

<http://studionumbersix.com/foo/lv2/schmooz-mono/ui>
	a		ui:GtkUI;
	ui:binary	<schmooz_ui.so> .


