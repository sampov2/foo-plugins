@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .


<http://studionumbersix.com/foo/lv2/limiter>
	a		lv2:LimiterPlugin;
	doap:name	"Foo Limiter";
	doap:license	<http://usefulinc.com/doap/licenses/gpl>;
	doap:maintainer [ foaf:name "Sampo Savolainen" ; ] ;


	lv2:port [
		a lv2:AudioPort, lv2:InputPort;
		lv2:index 0;
		lv2:symbol "input_l";
		lv2:name "Input L";
	],
	[
		a lv2:AudioPort, lv2:InputPort;
		lv2:index 1;
		lv2:symbol "input_r";
		lv2:name "Input R";
	],
	[
		a lv2:AudioPort, lv2:OutputPort;
		lv2:index 2;
		lv2:symbol "output_l";
		lv2:name "Output L";
	],
	[
		a lv2:AudioPort, lv2:OutputPort;
		lv2:index 3;
		lv2:symbol "output_r";
		lv2:name "Output R";
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 4;
		lv2:symbol "input_gain";
		lv2:name "Input gain";

		lv2:minimum -20.0;
		lv2:maximum  10.0;
		lv2:default   0.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 5;
		lv2:symbol "max_peak_db";
		lv2:name "Max level (dB)";

		lv2:minimum -30.0;
		lv2:maximum   0.0;
		lv2:default   0.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 6;
		lv2:symbol "release_time";
		lv2:name "Release (s)";

		lv2:minimum 0.01;
		lv2:maximum 2.0;
		lv2:default 0.1;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 7;
		lv2:symbol "release_scale";
		lv2:name "Linear/log release";

		lv2:minimum 0.0;
		lv2:maximum 1.0;
		lv2:default 0.75;
	],
  	[
		a lv2:ControlPort, lv2:OutputPort;
		lv2:index 8;
		lv2:symbol "attenuation";
		lv2:name "Attenuation (dB)";

		lv2:minimum   0.0;
		lv2:maximum +70.0;
	],
	[
		a lv2:ControlPort, lv2:OutputPort;
		lv2:index 9;
		lv2:symbol "latency";
		lv2:name "Latency";
		lv2:PortProperty lv2:reportsLatency;
	].

