@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://dublincore.org/documents/dcmi-namespace/> .

<http://studionumbersix.com/foo/lv2/saturator>
	a		lv2:DistortionPlugin;
	doap:name	"Foo Saturator";
	doap:license	<http://usefulinc.com/doap/licenses/gpl>;
	doap:maintainer [ foaf:name "Sampo Savolainen" ; ] ;
	dc:replaces <urn:ladspa:3187> ;

	lv2:port [
		a lv2:AudioPort, lv2:InputPort;
		lv2:index 0;
		lv2:symbol "input";
		lv2:name "Input";
	],
	[
		a lv2:AudioPort, lv2:OutputPort;
		lv2:index 1;
		lv2:symbol "output";
		lv2:name "Output";
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 2;
		lv2:symbol "gain";
		lv2:name "Input gain";

		lv2:minimum -20.0;
		lv2:maximum  20.0;
		lv2:default   0.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 3;
		lv2:symbol "max_peak_db";
		lv2:name "Max level (dB)";
		lv2:minimum -60.0;
		lv2:maximum   0.0;
		lv2:default   0.0;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 4;
		lv2:symbol "squarewaveness";
		lv2:name "Square-waveness";

		lv2:minimum 0.0;
		lv2:maximum 1.0;
		lv2:default 0.5;
	],
  	[
		a lv2:ControlPort, lv2:InputPort;
		lv2:index 5;
		lv2:symbol "drive";
		lv2:name "Drive";

		lv2:minimum 6.0;
		lv2:maximum 1.0;
		lv2:default 1.0;
	].

